{
  "name": "asset-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "test": "jest ./test",
    "lint": "eslint . --max-warnings 0",
    "compile": "node compile.js",
    "test:final": "npm run lint && npm run test"
  },
  "dependencies": {
    "bluebird": "~3.7.1",
    "body-parser": "~1.19.0",
    "cli-color": "~2.0.0",
    "commander": "~3.0.2",
    "compression": "~1.7.4",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "diff": "~4.0.1",
    "express": "~4.17.1",
    "http-shutdown": "~1.2.1",
    "morgan": "~1.9.1",
    "serve": "~11.2.0",
    "serve-favicon": "~2.5.0",
    "shelljs": "~0.8.3",
    "twig": "~1.13.3",
    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.5.1",
    "jest": "~24.9.0",
    "nodemon": "^1.19.3",
    "pkg": "^4.4.0",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "pkg": {
    "assets": "views/**/*",
    "bin": "server.js"
  },
  "defaultOptions": {
    "port": "7000",
    "dir": "./public",
    "log": "./asset-server-combined.log"
  }
}
