{
  "name": "asset-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "test": "jest ./test",
    "lint": "eslint . --max-warnings 0",
    "compile": "node compile.js",
    "test:final": "yarn run lint && yarn run test"
  },
  "dependencies": {
    "bluebird": "~3.5.1",
    "body-parser": "~1.18.2",
    "cli-color": "~1.2.0",
    "commander": "~2.15.1",
    "compression": "~1.7.2",
    "cookie-parser": "~1.4.3",
    "debug": "~3.1.0",
    "express": "~4.16.3",
    "http-shutdown": "~1.2.0",
    "jest": "~23.0.0-charlie.2",
    "morgan": "~1.9.0",
    "serve": "~10.0.2",
    "serve-favicon": "~2.5.0",
    "shelljs": "~0.8.1",
    "twig": "~1.11.0",
    "winston": "~2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "jest": "~23.0.0-charlie.2",
    "nodemon": "^1.17.3",
    "pkg": "^4.3.1",
    "superagent": "^3.8.0",
    "supertest": "^3.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "pkg": {
    "assets": "views/**/*",
    "bin": "server.js"
  },
  "defaultOptions": {
    "port": "7000",
    "dir": "./public",
    "log": "./asset-server-combined.log"
  }
}
